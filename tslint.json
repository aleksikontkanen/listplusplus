{
    "rules": {
        "class-name": true,
        "comment-format": true,
        "cyclomatic-complexity": [
            true,
            20
        ],
        "eofline": true,
        "indent": true,
        "interface-name": true,
        "interface-over-type-literal": true,
        "no-any": true,
        "no-eval": true,
        "no-shadowed-variable": true,
        "no-string-literal": true,
        "no-var-keyword": true,
        "only-arrow-functions": true,
        "prefer-const": true,
        "triple-equals": true,
        "no-duplicate-variable": true,
        "no-unused-variable": [
            true
        ]
    },
    "rulesDirectory": [
        "node_modules/tslint-eslint-rules/dist/rules"
    ]
}